}

.studentInfoIcon{
  align-items: center;
  justify-content: flex-end;
  display: flex;
  margin-right: 15rpx;
}
.search-view{
  height:90rpx;
}
.search-bar {
  position: relative;
  padding: 8px 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
  background-color: #f7f7f7;
}
.icon-search {
  margin-right: 8px;
  /*margin-top: 4px;*/
}
.icon-search-in-box {
  position: absolute;
  left: 10px;
  top: 7px;
}
.search-bar-text {
  display: inline-block;
  font-size: 14px;
  vertical-align: middle;
  font-family: "黑体";
}
.search-bar-form {
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: auto;
          flex: auto;
  border-radius: 5px;
  background: #FFFFFF;
  border: 1rpx solid #E6E6EA;
}
.search-bar-box {
  position: relative;
  padding-left: 30px;
  padding-right: 30px;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
}
.search-bar-input {
  height: 28px;
  line-height: 28px;
  font-size: 14px;
}
.icon-clear {
  position: absolute;
  top: 0;
  right: 0;
  padding: 7px 8px;
  font-size: 0;
}
.search-bar-label {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 2;
  border-radius: 3px;
  text-align: center;
  color: #9B9B9B;
  background: #FFFFFF;
  line-height: 28px;
}
.search-bar-cancel-btn {
  margin-left: 10px;
  line-height: 28px;
  color: #4fc3f7;
  white-space: nowrap;
}

.searchListView{
  height: 0;
}
.searchListView-show{
  height: 80%;
  width: 100%;
  position: absolute;
  z-index: 100;
  opacity: 1;
  background-color: #f7f7f7;
}
